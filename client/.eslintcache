uri: action.payload,\n            };\n\n    \n        case LOADING:\n            return {\n                ...state,\n                loading: true\n            };\n        \n        default:\n            return state\n    }\n}\n","/Users/nischal/Desktop/React/songify/client/src/components/cards/ArtistsCard.js",["97","98"],"import React, { Component } from 'react'\nimport { Link } from 'react-router-dom'\nimport unknown_artist from '../../assets/icons/user-not-found.png'\nimport { withRouter } from 'react-router';\n\n\nexport class ArtistCard extends Component {\n\n \n  componentWillReceiveProps(url){\n    var currentURL = url.match.params.id;\n    var newURL =  url.match.params.id;\n  }\n\n\n  render() {\n    const { artist } = this.props;\n    let artist_img = '';\n    artist_img = artist.images.length > 0 ? artist.images[0]['url'] : unknown_artist;\n    return (\n      <Link to={'/artist/' + artist.id } >\n        <div className=\"artists-container\">\n            <img src={artist_img} alt={artist.name}></img>\n            <h5>{artist.name}</h5>\n          </div>\n      </Link>\n    )\n  }\n\n}\n\nexport default withRouter(ArtistCard);\n","/Users/nischal/Desktop/React/songify/client/src/components/cards/AlbumsCard.js",[],"/Users/nischal/Desktop/React/songify/client/src/actions/types.js",[],["99","100"],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":34,"column":12,"nodeType":"107","messageId":"108","endLine":34,"endColumn":25},{"ruleId":"109","severity":1,"message":"110","line":18,"column":17,"nodeType":"111","endLine":18,"endColumn":73},{"ruleId":"112","severity":1,"message":"113","line":23,"column":1,"nodeType":"114","endLine":82,"endColumn":2},{"ruleId":"115","severity":1,"message":"116","line":11,"column":9,"nodeType":"117","messageId":"118","endLine":11,"endColumn":19},{"ruleId":"115","severity":1,"message":"119","line":12,"column":9,"nodeType":"117","messageId":"118","endLine":12,"e